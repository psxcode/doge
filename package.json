{
  "name": "@doge/doge",
  "version": "0.0.0",
  "homepage": "https://github.com/psxcode/doge",
  "repository": {
    "type": "git",
    "url": "https://github.com/psxcode/doge"
  },
  "author": "psxcode <psxcode@gmail.com> (https://github.com/psxcode)",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "lerna run test",
    "bootstrap": "lerna bootstrap",
    "commit": "git-cz",
    "build": "lerna run build",
    "release": "lerna publish --conventional-commits -m 'chore(release): publish'",
    "clean": "lerna run clean"
  },
  "devDependencies": {
    "@types/node": "*",
    "commitizen": "*",
    "cz-conventional-changelog": "*",
    "cz-customizable": "*",
    "lerna": "*",
    "rimraf": "*",
    "tslint": "*",
    "tslint-config-standard": "*",
    "typescript": "*"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    },
    "cz-customizable": {
      "config": "cs-customizable.config.js"
    }
  }
}
