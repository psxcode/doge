{
  "name": "@doge/fetch",
  "version": "0.0.0",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "psxcode <psxcode@gmail.com> (https://github.com/psxcode)",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn rollup",
    "clean": "rimraf dist",
    "tsc": "tsc",
    "rollup": "rollup -c rollup.config.js",
    "mocha": "mocha --opts mocha.opts",
    "test": "yarn test:once",
    "test:once": "nyc yarn mocha",
    "test:watch": "yarn mocha -- --watch",
    "test:coverage:generate": "nyc report --reporter=json",
    "test:coverage:report": "codecov -f coverage/*.json"
  },
  "dependencies": {
    "@doge/helpers": "*"
  },
  "devDependencies": {
    "@std/esm": "*",
    "@types/chai": "*",
    "@types/mocha": "*",
    "@types/node": "*",
    "@types/node-fetch": "*",
    "@types/sinon": "*",
    "@types/tough-cookie": "*",
    "chai": "*",
    "mocha": "*",
    "node-fetch": "*",
    "nyc": "*",
    "rimraf": "*",
    "rollup": "*",
    "rollup-plugin-commonjs": "*",
    "rollup-plugin-node-resolve": "*",
    "rollup-plugin-typescript2": "*",
    "sinon": "*",
    "source-map-support": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "all": true
  }
}
