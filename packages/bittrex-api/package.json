    "tough-cookie": "*"
  },
  "devDependencies": {
    "@std/esm": "*",
    "@types/chai": "*",
    "@types/mocha": "*",
    "@types/node": "*",
    "@types/node-fetch": "*",
    "@types/sinon": "*",
    "chai": "*",
    "mocha": "*",
    "nyc": "*",
    "rimraf": "*",
    "rollup": "*",
    "rollup-plugin-commonjs": "*",
    "rollup-plugin-node-resolve": "*",
    "rollup-plugin-typescript2": "*",
    "sinon": "*",
    "source-map-support": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
